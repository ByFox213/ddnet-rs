# IDE and editor specific files
.vs/                # Visual Studio workspace settings
.vscode/            # Visual Studio Code settings
.idea/              # JetBrains IDE settings

# Compiled files and executables
debug/              # Debug build output
target/             # Target directory for compiled artifacts

# Backup files generated by rustfmt
**/*.rs.bk          # Backup files for Rust source files

# MSVC Windows builds of rustc generate these, which store debugging information
*.pdb               # Program Database files for debugging

# Other, cache, analysis
.cache/             # Cache files
artifacts/          # Artifacts created by CI tests
*.sqlite            # SQLite database files
/trace.txt          # Artifact for the memory analysis feature
/flamegraph.svg     # Flamegraph output file
/perf.data*         # Performance data files
/callgrind.out.*    # Callgrind output files
